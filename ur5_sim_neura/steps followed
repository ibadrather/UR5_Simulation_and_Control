~/ur5_sim_neura$ source devel/setup.bash

~/ur5_sim_neura$ echo $ROS_PACKAGE_PATH

~/ur5_sim_neura/src $catkin_create_pkg ur5_sim controller_manager joint_state_controller robot_state_publisher std_msgs message_generation roscpp rospy sensor_msgs kdl_parser

~/ur5_sim_neura$ rosdep install --rosdistro kinetic --ignore-src --from-paths src


